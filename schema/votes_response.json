{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "VotesResponse",
  "type": "object",
  "required": [
    "count",
    "voters"
  ],
  "properties": {
    "count": {
      "type": "integer",
      "format": "uint64",
      "minimum": 0.0
    },
    "voters": {
      "type": "array",
      "items": {
        "type": "string"
      }
    }
  }
}
